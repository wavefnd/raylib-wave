import("../raylib");

fun main() {
    InitWindow(800, 600, "Wave + raylib");
    SetTargetFPS(60);

    var bg: Color = Color { r: 20, g: 20, b: 20, a: 255 };

    while (WindowShouldClose() == 0) {
        BeginDrawing();
        ClearBackground(bg);
        EndDrawing();
    }

    CloseWindow();
}