import("../raylib");

fun main() {
    InitWindow(800, 600, "Text");

    while (WindowShouldClose() == 0) {
        BeginDrawing();

        ClearBackground(Color { r: 0, g: 0, b: 0, a: 255 });
        DrawText("Hello from Wave", 200, 280, 24, Color { r: 255, g: 255, b: 255, a: 255 });

        EndDrawing();
    }

    CloseWindow();
}